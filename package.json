{
  "name": "NodeBoilerplate",
  "version": "1.0.0",
  "main": "main.js",
  "repository": "https://github.com/ilyaskarim/NodeBoilerplate.git",
  "author": "IlyasKarim <ilyas.datoo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon main --exec \"node -r dotenv/config -r babel-register\"",
    "db:migrate": "cross-env NODE_ENV=development nodemon sync --exec \"node -r dotenv/config -r babel-register\"",
    "generate": "plop --plopfile framework/generators/index.js --require babel-core/register"
  },
  "_moduleAliases": {
    "@": ".",
    "@framework": "framework/",
    "@app": "app/"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-node5": "^12.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "faker": "^4.1.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.0",
    "handlebars": "^4.0.12",
    "http-status-codes": "^1.3.0",
    "joi": "^14.3.1",
    "js-beautify": "^1.8.9",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "module-alias": "^2.1.0",
    "moment": "^2.23.0",
    "mongoose": "^5.4.2",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.4",
    "nodemailer": "^5.0.0",
    "nodemon": "^1.18.9",
    "plop": "^2.2.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "validate.js": "^0.12.0",
    "winston": "^3.1.0"
  },
  "dependencies": {
    "apollo-server": "^2.3.2"
  }
}
